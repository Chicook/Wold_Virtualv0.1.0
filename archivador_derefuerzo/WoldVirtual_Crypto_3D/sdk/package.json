{
  "name": "@woldvirtual/sdk",
  "version": "0.1.0",
  "description": "SDK oficial para el metaverso WoldVirtual Crypto 3D",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js}",
    "docs": "typedoc src/index.ts --out docs",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "metaverso",
    "3d",
    "blockchain",
    "nft",
    "web3",
    "threejs",
    "virtual-world",
    "crypto",
    "gaming"
  ],
  "author": "WoldVirtual Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/woldvirtual/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/woldvirtual/sdk/issues"
  },
  "homepage": "https://docs.woldvirtual.com/sdk",
  "dependencies": {
    "three": "^0.158.0",
    "web3": "^4.2.2",
    "ethers": "^6.8.1",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "ipfs-http-client": "^60.0.1",
    "arweave": "^1.14.4",
    "socket.io-client": "^4.7.4",
    "cannon-es": "^0.20.0",
    "gltf-pipeline": "^3.0.5",
    "draco3d": "^1.5.7",
    "ktx-parse": "^0.4.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "axios": "^1.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/three": "^0.158.3",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "rollup": "^4.6.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-dts": "^6.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "three": ">=0.150.0",
    "web3": ">=4.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
} 